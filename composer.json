    "repositories": [
        {
            "type": "package",
            "package": {
                "version": "3.1.1",
                "name": "bootstrap",
                "source": {
                    "url": "https://github.com/twitter/bootstrap.git",
                    "type": "git",
                    "reference": "v3.1.1"
                },
                "dist": {
                    "url": "https://github.com/twitter/bootstrap/releases/download/v3.1.1/bootstrap-3.1.1-dist.zip",
