                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "jquery",
                "version": "2.1.0",
                "source": {
                    "url": "https://github.com/jquery/jquery",
                    "type": "git",
                    "reference": "2.1.0"
                },
                "dist": {
                    "url": "http://code.jquery.com/jquery-2.1.0.min.js",
                    "type": "file"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "bootstrap": "3.1.1",
        "jquery": "2.1.0"
